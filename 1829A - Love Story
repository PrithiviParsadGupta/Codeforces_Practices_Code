#include<bits/stdc++.h>
using namespace std;
int main()
{
    int testcases;
    cin>>testcases;
    while(testcases--)
    {
    int count=0;
    string s1;
    cin>>s1;
    string s2="codeforces";
    for(int i=0;i<10;i++)
    {
        if(s1[i]!=s2[i])
        count++;
    }
    cout<<count<<endl;
}
}
